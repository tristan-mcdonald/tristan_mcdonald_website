{
    "env": {
        "browser": true,
        "jest": true,
        "jquery": false,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "rules": {
        "camelcase": 0,
        "comma-dangle": [
            "error",
            "always-multiline"
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-multi-spaces": 0,
        "quotes": [
            1,
            "double"
        ],
        "prefer-const": "error",
        "semi": [
            "error",
            "always"
        ],
        "sort-vars": "error",
        "space-before-function-paren": [
            "error",
            "always"
        ]
    }
}
